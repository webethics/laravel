.langdropdown a.dropdown-item img {
    width: 30px;
    margin-right: 10px;
}
.langdropdown a.nav-link.dropdown-toggle {
    display: inline;
}
.langdropdown .dropdown-menu {
    margin-top: 6px;
    background: #1F44B8;
    border: 0;
    border-radius: 0px 0px 10px 10px;
    -webkit-box-shadow: 3px 10px 20px 0px rgba(0, 0, 0, 0.4);
    box-shadow: 3px 10px 20px 0px rgba(0, 0, 0, 0.4);
}
.langdropdown .dropdown-menu .dropdown-item{color:#fff;}
li.nav-item.dropdown.langdropdown .dropdown-item:hover{color:#1f44b8;}

li.nav-item.dropdown.langdropdown .dropdown-item.active_lang {
    color: #1f44b8; background: #fff;
}

li.nav-item.dropdown.langdropdown a:after {
    right: 0;
    position: absolute;
    top:12px;
}

.langdropdown a.dropdown-item img {
    float: left;
}


